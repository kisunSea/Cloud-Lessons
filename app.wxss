/**app.wxss**/

/* @import "style/weui.wxss"; */
@import './style/weui-miniprogram/weui-wxss/dist/style/weui.wxss';

.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
  /* background-color: #f7f7f7; */
} 


/*自定义tabbar样式开始*/
.tabbar_box{
    display: flex;
    flex-direction: row;
    justify-content: space-around;
    position: fixed;
    bottom: 0;
    left: 0;
    z-index: 999;
    width: 100%;
    height: 120rpx;
    border-top: 1rpx solid #d7d7d7; 
}

.tabbar_nav{
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
    font-size: 28rpx;
    height: 100%;
}

.tabbar_icon{
    width: 50rpx;
    height:50rpx;
}

.tabbar_nav:nth-child(2) image{ 
    position:relative;
    top:-32rpx;
    width:80rpx;
    height:80rpx;
}
.tabbar_nav:nth-child(2) text{  
    position:relative; 
    top:-32rpx;
}
/*自定义tabbar样式结束*/


/*iconfont字体文件引入开始*/
@font-face {
  font-family: 'iconfont';  /* project id 1488649 */
  src: url('//at.alicdn.com/t/font_1488649_j8l76r91rf.eot');
  src: url('//at.alicdn.com/t/font_1488649_j8l76r91rf.eot?#iefix') format('embedded-opentype'),
  url('//at.alicdn.com/t/font_1488649_j8l76r91rf.woff2') format('woff2'),
  url('//at.alicdn.com/t/font_1488649_j8l76r91rf.woff') format('woff'),
  url('//at.alicdn.com/t/font_1488649_j8l76r91rf.ttf') format('truetype'),
  url('//at.alicdn.com/t/font_1488649_j8l76r91rf.svg#iconfont') format('svg');
}

.iconfont{
    
    font-style:normal;
    -webkit-font-smoothing: antialiased;
    -webkit-text-stroke-width: 0.2px;
    -moz-osx-font-smoothing: grayscale;
}
/* iconfont字体文件引入结束*/


